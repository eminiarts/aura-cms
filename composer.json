{
    "name": "eminiarts/aura-cms",
    "description": "Aura CMS for Laravel",
    "keywords": [
        "Aura",
        "Aura CMS",
        "Laravel Aura",
        "Laravel",
        "Livewire",
        "TALL",
        "TALL Stack",
        "TailwindCSS",
        "AlpineJS",
        "Emini Arts",
        "eminiarts"
    ],
    "homepage": "https://github.com/eminiarts/aura-cms",
    "license": "MIT",
    "authors": [{
        "name": "Emini Arts GmbH",
        "email": "support@eminiarts.ch",
        "role": "Developer"
    }],
    "require": {
        "php": "^8.2",
        "doctrine/dbal": "^3.6|^4.0",
        "intervention/image": "^2.7|^3.0",
        "laravel/fortify": "^1.20",
        "lab404/laravel-impersonate": "^1.7.5",
        "laravel/prompts": "^0.3",
        "laravel/sanctum": "^4.0",
        "livewire/livewire": "^3.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/dusk": "^8.0",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "laravel/pint": "^1.18",
        "nunomaduro/collision": "^8.1",
        "nunomaduro/larastan": "^2.0|^3.0",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.2|^3.0",
        "pestphp/pest-plugin-livewire": "^2.1|^3.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0|^2.0",
        "spatie/laravel-ray": "^1.32",
        "spatie/test-time": "^1.3",
        "spatie/ray": "^1.37"
    },
    "autoload": {
        "psr-4": {
            "Aura\\Base\\": "src",
            "Aura\\Base\\Database\\Factories\\": "database/factories",
            "Aura\\Forms\\": "plugins/aura/forms/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aura\\Base\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest --no-coverage --parallel",
        "test-coverage": "vendor/bin/pest --coverage --parallel --min=80",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lab404\\Impersonate\\ImpersonateServiceProvider",
                "Aura\\Base\\Providers\\AuthServiceProvider",
                "Aura\\Base\\Providers\\AppServiceProvider",
                "Aura\\Base\\AuraServiceProvider",
                "Aura\\Forms\\FormsServiceProvider"
            ],
            "aliases": {
                "Aura": "Aura\\Base\\Facades\\Aura"
            }
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
